{
    "name": "josepostiga/jwt-bouncer",
    "description": "A JWT authorization guard for your Laravel/Lumen apps.",
    "keywords": ["php", "package", "laravel", "lumen", "jwt-bouncer", "jwt", "authorization"],
    "homepage": "https://github.com/josepostiga/jwt-bouncer",
    "license": "MIT",
    "authors": [
        {
            "name": "Jos√© Postiga",
            "email": "iamjosepostiga@gmail.com",
            "homepage": "https://josepostiga.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.4|^8.0",
        "lcobucci/jwt": "^3.3|^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.4",
        "php-coveralls/php-coveralls": "^2.2",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "JosePostiga\\JwtBouncer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JosePostiga\\JwtBouncer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JosePostiga\\JwtBouncer\\JwtBouncerServiceProvider"
            ]
        }
    }
}
